source "https://rubygems.org"

# Run Jekyll with 'bundle exec jekyll serve'

# To upgrade, run 'bundle update github-pages'
gem "github-pages", group: :jekyll_plugins

group :jekyll_plugins do
  gem 'jekyll-sitemap'
  gem 'jekyll-feed'
  gem 'jekyll-seo-tag'
end

gem 'wdm', '>= 0.1.0' if Gem.win_platform?
gem "webrick", "~> 1.7"
